class Solution(object):
    def countOperations(self, num1, num2):
        """
        :type num1: int
        :type num2: int
        :rtype: int
        """ 
        if num1 == 0 or num2 == 0:
            return 0
        if num1 == num2:
            return 1
        c = 1
        while num1!=num2:
            if num1>num2:
                num1-=num2
            else:
                num2-=num1
            c+=1
        return c
